{
  "name": "burst-vtm-dashboard",
  "version": "1.0.0",
  "description": "Visual Testing Module Dashboard",
  "engines": {
    "npm": ">=3",
    "node": "6.9.1"
  },
  "scripts": {
    "start": "npm-run-all --parallel serve:src lint:watch",
    "lint": "rimraf ./public && esw webpack.config.* src tools data --color --fix --config .eslintrc",
    "lint:watch": "npm run lint -- --watch",
    "build": "rimraf ./public && babel-node tools/build.js",
    "serve:src": "rimraf ./public && nodemon $NODE_DEBUG_OPTION ./tools/srcServer.js --exec babel-node --config ./nodemon.json",
    "serve:public": "NODE_ENV=production babel-node tools/distServer.js",
    "postinstall": "npm run build"
  },
  "author": "Burst",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "6.5.1",
    "babel-cli": "6.16.0",
    "babel-core": "6.17.0",
    "babel-eslint": "7.0.0",
    "babel-loader": "6.2.5",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-plugin-transform-react-constant-elements": "6.9.1",
    "babel-plugin-transform-react-remove-prop-types": "0.2.10",
    "babel-polyfill": "^6.16.0",
    "babel-preset-latest": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-1": "6.16.0",
    "babel-register": "6.16.3",
    "body-parser": "1.15.2",
    "bootstrap": "^3.3.7",
    "browser-sync": "2.17.5",
